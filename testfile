<p id="productList"></p>
<form>
<script>
var products = ["Milk", "Bread", "IceCream", "Fruit", "Meat", "Potato"];
var description = "The following products are available in the supermarket: <br>";
var i;
for (i = 0; i < products.length; i++) {
  description += '<input type="checkbox" id="vehicle1" name="vehicle1" value="Bike">' + products[i] + "<br>"

}
document.getElementById("productList").innerHTML = description;
</script>



